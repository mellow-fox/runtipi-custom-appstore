{
  "services": [
    {
      "name": "photon",
      "image": "rtuszik/photon-docker:0.7.2",
      "environment": {
        "UPDATE_STRATEGY": "PARALLEL",
        "UPDATE_INTERVAL": "720h"
      },
      "internalPort": 2322,
      "volumes": [
        {
          "hostPath": "photon_data",
          "containerPath": "/photon/photon_data",
          "readOnly": false,
          "shared": false,
          "private": false
        }
      ]
    }
  ]
}